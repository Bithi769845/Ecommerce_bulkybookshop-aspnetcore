@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1 class="text-white">Index</h1>

<table class="table table-bordered table-striped text-white" style="width:100%;">
    <thead>
		<tr>
			<th>Category Name</th>
			<th>Display Order</th>
		</tr>
    </thead>

	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Name</td>
				<td>@item.DisplayOrder</td>
			</tr>
		}
	</tbody>

</table>